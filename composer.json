{
    "name": "fw4/whise-webservices",
    "type": "library",
    "description": "PHP library for implementing WHISE Webservices",
    "keywords": ["whise", "whoman", "webservices", "api", "sdk"],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "~6.0",
        "ocramius/package-versions": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5 || ^6.0"
    },
    "autoload": {
        "psr-4": { "Whise\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Whise\\Tests\\": "tests/"}
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests/WebServicesTest"
    },
    "config": {
        "optimize-autoloader": true
    }
}
